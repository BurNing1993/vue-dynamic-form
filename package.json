{
  "name": "vue-dynamic-questionnaire",
  "version": "0.1.1",
  "license": "MIT",
  "main": "/dist/vue-dynamic-form.common.js",
  "homepage": "https://burning1993.github.io/vue-dynamic-questionnaire",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "keywords": [
    "vue-dynamic-questionnaire",
    "vue",
    "vue-dynamic-form"
  ],
  "author": "Joey",
  "dependencies": {
    "core-js": "^2.6.5",
    "element-ui": "^2.12.0",
    "vue": "^2.6.10",
    "vue-dynamic-questionnaire": "^0.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.5",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "max-len": [
        "error",
        {
          "code": 150
        }
      ],
      "array-callback-return": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
